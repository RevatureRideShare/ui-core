#!/bin/bash

pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                sh 'echo "Hello World"'
                sh '''
                    echo "I am building RideShare Angular"
		    pwd
                    echo "Where is node"
                    nvm which node
                    node -v
                    npm -v
		    npm install
                    ng build
                '''
            }
        }
    }
}