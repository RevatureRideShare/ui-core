#!/bin/bash

pipeline {
    agent any
    stages {
        stage('Debugging') {
            steps {
                sh 'echo "Hello World"'
                sh '''
                    echo "Multiline shell steps works too"
		    . ~/.nvm/nvm.sh
                    pwd
                    echo "Where is node"
                    nvm which node
                    node -v
                    npm -v
                    ls -lah
                '''
            }
        }
    }
}